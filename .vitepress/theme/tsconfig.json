{
	"compilerOptions": {
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	"isolatedModules": true,
	"skipLibCheck": true,
	"module": "ESNext",
	"target": "ES2020",
	"moduleResolution": "Bundler",
	"types": ["vite/client"]
	},
	"exclude": ["node_modules"],
	"include": [
		"components/**/*.ts",
		"utils/**/*.vue",
		"./*.ts"
	]
}
